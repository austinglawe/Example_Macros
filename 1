Public Sub Format_Cells()

    Dim CurrentSheet As Worksheet
    Dim LastRow As Long
    
    ' Set the Current Worksheet to the variable 'CurrentSheet'
        Set CurrentSheet = ActiveWorkbook.ActiveSheet
    
    ' Find the last row of data in column A
        LastRow = CurrentSheet.Cells(CurrentSheet.Rows.Count, 1).End(xlUp).Row
        
    ' Fill Formula from D2 down to the end of the data
        CurrentSheet.Range("D2:D" & LastRow).FillDown
        
    ' In Row 1 (Row with Headers) Change Font to Bold, Italic, Underlined, Size 18, Arial Style, and Yellow. Then make the cells' background color black
        With CurrentSheet.Range("A1:E1").Font
            .Bold = True
            .Italic = True
            .Underline = xlUnderlineStyleSingle
            .Size = 18
            .Name = "Arial"
            .Color = RGB(255, 255, 0)
        End With
        
        CurrentSheet.Range("A1:E1").Interior.Color = RGB(0, 0, 0)
        
    ' AutoFit the Columns
        CurrentSheet.Columns("A:E").AutoFit
        
    
End Sub

