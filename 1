Sub Run_All_Subs()

    ' Run the 'Create_New_Workbook' Sub
        Create_New_Workbook
    
    ' Run the 'Rename_Sheet' Sub
        Rename_Sheet
    
    ' Run the 'Add_Headers' Sub
        Add_Headers
    
    ' Run 'Add_Contents' Sub
        Add_Contents
    
    ' Run 'Format_Cells' Sub
        Format_Cells

End Sub
Public Sub Create_New_Workbook()

    Dim NewWorkbook As Workbook

    ' Add a new workbook
        Set NewWorkbook = Workbooks.Add
    
    ' Select the New Workbook
        NewWorkbook.ActiveSheet.Activate
    

End Sub

Public Sub Rename_Sheet()

    Dim CurrentSheet As Worksheet
    
    ' Set the Current Worksheet to the variable 'CurrentSheet'
        Set CurrentSheet = ActiveWorkbook.ActiveSheet
    
    ' Rename the current worksheet
        CurrentSheet.Name = "Renamed Sheet"

End Sub
Public Sub Add_Headers()

    Dim CurrentSheet As Worksheet
    
    ' Set the Current Worksheet to the variable 'CurrentSheet'
        Set CurrentSheet = ActiveWorkbook.ActiveSheet
    
    ' Create Headers in Row 1
        CurrentSheet.Range("A1").Value = "First Name"
        CurrentSheet.Range("B1").Value = "Last Name"
        CurrentSheet.Range("C1").Value = "Birth Date"
        CurrentSheet.Range("D1").Value = "Current Age (Days)"
        CurrentSheet.Range("E1").Value = "Favorite Color"

End Sub
Public Sub Add_Contents()

    Dim CurrentSheet As Worksheet
    
    ' Set the Current Worksheet to the variable 'CurrentSheet'
        Set CurrentSheet = ActiveWorkbook.ActiveSheet
    
    ' Create the data (Columns: A - First Name, B- Last Name, C- Birthday, D- Current Age, E- Favorite Color)
        ' Liam Smith (Row 2)
            CurrentSheet.Range("A2").Value = "Liam"
            CurrentSheet.Range("B2").Value = "Smith"
            CurrentSheet.Range("C2").Value = "03/02/1975"
            CurrentSheet.Range("D2").Formula2 = "=TEXT(TODAY()-C2,""0"")"
            CurrentSheet.Range("E2").Interior.Color = vbBlue
            ' CurrentSheet.Range("E2").Interior.Color = RGB(0, 0, 255)
            CurrentSheet.Range("E2").Value = "Blue"
            CurrentSheet.Range("E2").Font.Color = vbWhite
            
        
        ' Olivia Wang (Row 3)
            CurrentSheet.Range("A3").Value = "Olivia"
            CurrentSheet.Range("B3").Value = "Wang"
            CurrentSheet.Range("C3").Value = "09/19/1989"
            CurrentSheet.Range("E3").Interior.Color = vbRed
            CurrentSheet.Range("E3").Value = "Red"
        
        ' Noah Miller (Row 4)
            CurrentSheet.Range("A4").Value = "Noah"
            CurrentSheet.Range("B4").Value = "Miller"
            CurrentSheet.Range("C4").Value = "10/01/2002"
            CurrentSheet.Range("E4").Interior.Color = vbGreen
            CurrentSheet.Range("E4").Value = "Green"
        
        ' James Garcia (Row 5)
            CurrentSheet.Range("A5").Value = "James"
            CurrentSheet.Range("B5").Value = "Garcia"
            CurrentSheet.Range("C5").Value = "08/30/1943"
            CurrentSheet.Range("E5").Interior.Color = vbCyan
            CurrentSheet.Range("E5").Value = "Cyan"
        
        ' Emma Li (Row 6)
